  "Channels": ["Microsoft-Windows-Sysmon/Operational"],
  "Computers": [],
  "Traces": [
    "*::ProcessGuid = ProcessGuid",
    "::ParentProcessGuid = ProcessGuid"
  ],
  "Criticality": 10,
  "Author": "@0xrawsec",
  "Comment": "Rule that triggers on malicious use of regsvr32.exe (see:Â metasploit regsvr32_applocker_bypass_server exploit)"
  },
"Matches": [
  "$im: Image ~= '(?i:^c:\\\\windows\\\\system32\\\\regsvr32.exe$)'",
  "$sw1: CommandLine ~= '(?i: /n )'",
